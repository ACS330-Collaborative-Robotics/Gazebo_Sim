	<!-- Default Inertial -->
	<xacro:macro name="default_inertial" params="z_value mass">
		<inertial>
			<origin xyz="0 0 ${z_value}" rpy="0 0 0"/>
			<mass value="${mass}" />
			<inertia ixx="0.1" ixy="0.0" ixz="0.0"
               iyy="0.1" iyz="0.0"
               izz="0.1" />
		</inertial>
	</xacro:macro>


