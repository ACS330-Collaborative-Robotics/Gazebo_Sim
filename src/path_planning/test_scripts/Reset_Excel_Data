from openpyxl import Workbokk, load_workbook
wb = load_workbook('home/stevencraig147/catkin_ws/src/path_plannning/test_scripts/Testing.xlsx')
wobj = wb['Objects']
wroute = wb['Route']
for column in range(1,3):
    wobj.delete_cols(column)
    wroute.delete_cols(column)

wb.save('home/stevencraig147/catkin_ws/src/path_plannning/test_scripts/Testing.xlsx')